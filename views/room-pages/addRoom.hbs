{{!-- If in the form you want to upload any kind og files
images/ music / etc ... you need to add enctype="multipart/form-data" --}}

<form action="/create/room" method="POST" enctype="multipart/form-data">

  <label for="name">Name</label>
  <input type="text" name="name" id="">
  <label for="description">Description</label>
  <textarea name="description" id="" cols="30" rows="10"></textarea>

  <label for="">Upload Image</label>
  <input type="file" name="imageUrl" id="">

  <button type="submit">Create Room</button>

</form>